# OpenAI API key for authenticating requests to the OpenAI service
OPENAI_API_KEY=YOUR_OPENAI_API_KEY_HERE

# PostgreSQL database connection URL
DATABASE_URL=postgresql://username:password@localhost/dbname

# JWT secret used for signing and verifying tokens for authentication
JWT_SECRET=YOUR_JWT_SECRET_HERE

# Environment type (development, testing, production)
ENV=development

# Port where the application will run
PORT=8000